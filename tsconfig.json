{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts", "index.ts", "watchProduct/watchProduct.ts"
  ]
}
